{
	"type": "origins:multiple",
	"name": "Stunted",
	"description": "Goblins' poor diets and genetics prevent them from growing as tall as other creatures.",
	"stunted": {
		"type": "origins:action_on_callback",
		"entity_action_chosen": {
			"type": "origins:execute_command",
			"command": "scale set pehkui:base 0.8"
		},
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "scale reset pehkui:base"
		},
		"execute_chosen_when_orb": true
	},
	"stunted2": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:execute_command",
			"command": "scale set pehkui:motion 1.1"
		},
		"interval": 100
	},
	"stunted3": {
		"type": "origins:action_on_callback",
		"entity_action_chosen": {
			"type": "origins:execute_command",
			"command": "scale persist set pehkui:base true @s"
		},
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "scale persist reset pehkui:base true @s"
		},
		"execute_chosen_when_orb": true
	},
	"stunted4": {
		"type": "origins:action_on_callback",
		"entity_action_chosen": {
			"type": "origins:execute_command",
			"command": "scale set pehkui:motion 1.1"
		},
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "scale reset pehkui:motion"
		},
		"execute_chosen_when_orb": true
	}
}