{
	"type": "origins:multiple",
	"name": "Towering",
	"description": "Elves are substantially taller than the average human. Despite this, they are just as skinny...",
	"towering": {
		"type": "origins:action_on_callback",
		"entity_action_chosen": {
			"type": "origins:execute_command",
			"command": "scale set pehkui:height 1.2"
		},
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "scale reset pehkui:height"
		},
		"execute_chosen_when_orb": true
	},
	"towering2": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:execute_command",
			"command": "scale set pehkui:height 1.2"
		},
		"interval": 100
	}
}